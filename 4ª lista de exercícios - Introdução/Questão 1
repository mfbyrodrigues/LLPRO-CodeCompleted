#include <stdio.h>

// Faça um programa na linguagem C para converter um valor real em dólar. Considere 1 real = 6 dólares.

int main () {

	float real, d;

	printf ("Digite o valor em reais: \n");
	scanf ("%f", &real);
	d = (real*6);
	printf ("O novo valor em dólar é de: %.2f \n", d);

	return 0;
}
